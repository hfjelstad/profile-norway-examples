<?xml version="1.0" encoding="UTF-8"?>
<!--
DestinationDisplay is used to specify text that is advertized to public, both on the screens on vehicle sides and on some other media
-->
<ServiceFrame xmlns="http://www.netex.org.uk/netex"
  xmlns:siri="http://www.siri.org.uk/siri"
  xmlns:core="http://www.govtalk.gov.uk/core"
  xmlns:ifopt="http://www.ifopt.org.uk/ifopt"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    
    <destinationDisplays>
        <!-- Metro Line 5 to Sognsvann -->
        <DestinationDisplay version="1" id="RUT:DestinationDisplay:5-sognsvann">
            <SideText>5 Sognsvann</SideText>
            <FrontText>5 Sognsvann</FrontText>
            <PublicCode>5</PublicCode>
            <vias>
                <Via>
                    <DestinationDisplayRef ref="RUT:DestinationDisplay:Ringen" version="1"/>
                    <ViaType>name</ViaType>
                </Via>
            </vias>
            <variants>
                <DestinationDisplayVariant>
                    <DestinationDisplayVariantMediaType>web</DestinationDisplayVariantMediaType>
                    <FrontText>Sognsvann</FrontText>
                </DestinationDisplayVariant>
            </variants>
        </DestinationDisplay>
        
        <!-- Metro Line 5 to Vestli -->
        <DestinationDisplay version="1" id="RUT:DestinationDisplay:5-sognsvann">
            <SideText>5 Vestli</SideText>
            <FrontText>5 Vestli</FrontText>
            <PublicCode>5</PublicCode>
            <vias>
                <Via>
                    <DestinationDisplayRef ref="RUT:DestinationDisplay:Ringen" version="1"/>
                    <ViaType>name</ViaType>
                </Via>
            </vias>
            <variants>
                <DestinationDisplayVariant>
                    <DestinationDisplayVariantMediaType>web</DestinationDisplayVariantMediaType>
                    <FrontText>Vestli</FrontText>
                </DestinationDisplayVariant>
            </variants>
        </DestinationDisplay>
        
        <!-- Via -->
        <DestinationDisplay version="1" id="RUT:DestinationDisplay:Ringen">
            <FrontText>Ringen</FrontText>
        </DestinationDisplay>
    </destinationDisplays>
</ServiceFrame>